{
  "name": "template-cdk-lambda-base",
  "version": "0.1.0",
  "bin": {
    "template-cdk-lambda-base": "bin/template-cdk-lambda-base.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "build:dev": "tsc-watch --onSuccess \"./start-api.sh\"",
    "lint": "eslint . --ext .ts",
    "lint:watch": "esw -w",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "sam:local-api": "AWS_DEFAULT_REGION=eu-west-1 NODE_ENV=local sam local start-api -t ./cdk.out/pumped-main.template.json --skip-pull-image --host localhost -p 6000 -n local/env.json --log-file logfile.txt",
    "load-draw-io-file": "cfn-dia draw.io -t ./cdk.out/pumped-main.template.json ",
    "dev": "IS_SAM_LOCAL=true MUTATION_TOKEN_SECRET=SECRET cdk synth && concurrently \"npm run docker:start --watch\"  \"npm run sam:local-api\"",
    "dev:debug": "IS_SAM_LOCAL=true MUTATION_TOKEN_SECRET=SECRET cdk synth && concurrently \"npm run docker:start --watch\" \"npm run sam:local-api --debug\"",
    "watch": "tsc -w",
    "test": "jest",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write '**/*.ts'",
    "synth": "cdk synth -a 'ts-node ./bin/template-cdk-lambda-base.ts' --no-staging --no-validation --quiet --output cdk.local"
  },
  "jest-junit": {
    "suiteName": "Jest unit test",
    "outputDirectory": "./report",
    "outputName": "./jest-junit.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
